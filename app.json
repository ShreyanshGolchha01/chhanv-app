{
  "expo": {
    "name": "Project Chhanv",
    "slug": "GovtHealthAppExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chhanv_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/loading_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chhanv_logo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shreyansh_golchha1112.GovtHealthAppExpo",
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "./network_security_config.xml",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 21,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https"
            },
            {
              "scheme": "http"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "pulkita_12",
    "extra": {
      "eas": {
        "projectId": "b273705d-abf9-4aa1-99a1-4587c6cf47ff"
      }
    }
  }
}
